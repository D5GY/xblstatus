{
  "compilerOptions": {
    "target": "ESNext",
    "strict": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "declaration": false,
    "removeComments": true,
    "alwaysStrict": true,
    "pretty": true,
    "outDir": "./build",
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules"
  ]
}